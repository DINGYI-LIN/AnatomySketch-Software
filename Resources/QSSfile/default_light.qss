/* 全局字体 */
* {
	font-family: Helvetica, 'Hiragino Sans GB', 'Microsoft Yahei', Arial, sans-serif;
}


/* 按钮 */
QPushButton {
    background-color: #E4E4E4;
    border: 1px solid #808080;
    color: #3F3F3F;
    border-radius: 4px;
    padding: 3px;
    outline: none;
}

QPushButton:disabled {
    background-color: #E4E4E4;
    border: 1px solid #808080;
    color: #787878;
    border-radius: 4px;
    padding: 3px;
}

QPushButton:checked {
    background-color: #808080;
    border: 1px solid #808080;
    border-radius: 4px;
    padding: 3px;
    outline: none;
}

QPushButton:checked:disabled {
    background-color: #19232D;
    border: 1px solid #808080;
    color: #787878;
    border-radius: 4px;
    padding: 3px;
    outline: none;
}

QPushButton::menu-indicator {
    subcontrol-origin: padding;
    subcontrol-position: bottom right;
    bottom: 4px;
}

QPushButton:pressed {
    background-color: #D4D4D4;
    border: 1px solid #19232D;
}

QPushButton:hover,
QPushButton:checked:hover{
    border: 1px solid #148CD2;
    color: #3A3A3A;
}

QPushButton:selected,
QPushButton:checked:selected{
    background: #1464A0;
    color: #808080;
}


/* 打开 */
QPushButton#BtnOpen {
    background: transparent;
	color:rgb(102,102,102);
    font:bold 14px;
    border:none;
    min-width:62px;
    min-height:12px;
    padding:4px;
}
QPushButton#BtnOpen:pressed {
    border:1px solid;
	border-color:rgb(135,206,250);
    background-color:rgb(173,216,230);
    padding-left:5px;
    padding-top:5px;
}
QPushButton#BtnOpen:hover {
	border:1px solid;
	border-color:rgb(135,206,250);
    background-color:rgb(173,216,230);
}


