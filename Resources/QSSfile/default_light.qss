    background-color: #148CD2;
    border: 1px solid #19232D;
    color: #19232D;
    padding: 0;   
    opacity: 230; 
}


/* 复选框 */
QCheckBox {
    background-color: #F5F5F5;
    color: #3F3F3F;
    spacing: 4px;
    outline: none;
    padding-top: 4px;
    padding-bottom: 4px;
	padding-left: -5px;
}

QCheckBox:focus {
    border: none;
}

QCheckBox QWidget:disabled {
    background-color: #F5F5F5;
    color: #787878;
}

QCheckBox::indicator {
    margin-left: 4px;
    width: 16px;
    height: 16px;
}

QCheckBox::indicator:unchecked {
    image: url(:/Icon/checkbox_unchecked);
}

QCheckBox::indicator:unchecked:hover,
QCheckBox::indicator:unchecked:focus,
QCheckBox::indicator:unchecked:pressed {
    border: none;
    image: url(:/Icon/checkbox_unchecked_focus);
}

QCheckBox::indicator:unchecked:disabled {
    image: url(:/Icon/checkbox_unchecked_disabled);
}

QCheckBox::indicator:checked {
    image: url(:/Icon/checkbox_checked);
}

QCheckBox::indicator:checked:hover,
QCheckBox::indicator:checked:focus,
QCheckBox::indicator:checked:pressed {
    border: none;
    image: url(:/Icon/checkbox_checked_focus);
}

QCheckBox::indicator:checked:disabled{
    image: url(:/Icon/checkbox_checked_disabled);
}

QCheckBox::indicator:indeterminate {
    image: url(:/Icon/checkbox_indeterminate);
}

QCheckBox::indicator:indeterminate:disabled {
    image: url(:/Icon/checkbox_indeterminate_disabled);
}

QCheckBox::indicator:indeterminate:focus,
QCheckBox::indicator:indeterminate:hover,
QCheckBox::indicator:indeterminate:pressed {
    image: url(:/Icon/checkbox_indeterminate_focus);
}


/* 组框 */
QGroupBox {
    font-weight: bold;
    border: 1px solid #808080;
    border-radius: 4px;
    padding: 4px;
    margin-top: 16px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 3px;
    padding-left: 3px;
    padding-right: 5px;
    padding-top: 8px;
    padding-bottom: 16px;
}

QGroupBox::indicator {
    margin-left: 4px;
    width: 16px;
    height: 16px;
}

QGroupBox::indicator {
    margin-left: 2px;
}

QGroupBox::indicator:unchecked:hover,
QGroupBox::indicator:unchecked:focus,
QGroupBox::indicator:unchecked:pressed {
    border: none;
    image: url(:/Icon/checkbox_unchecked_focus);
}

QGroupBox::indicator:checked:hover,
QGroupBox::indicator:checked:focus,
QGroupBox::indicator:checked:pressed {
    border: none;
    image: url(:/Icon/checkbox_checked_focus);
}

QGroupBox::indicator:checked:disabled {
    image: url(:/Icon/checkbox_checked_disabled);
}

QGroupBox::indicator:unchecked:disabled {
    image: url(:/Icon/checkbox_unchecked_disabled);
}


/* 工具条 */
QToolBar {
    background-color:#F5F5F5;
    padding: 2px;
    font-weight: bold;
}

QToolBar QToolButton{
    background-color:#F5F5F5;
    margin-top: 2px;
    margin-bottom: 2px;
    margin-right: 2px;
}

QToolBar QToolButton:checked{
    background-color: #EEEEEE;
    border: 0px solid #32414B;
    border-radius: 0px;
    margin-top: 2px;
    margin-bottom: 2px;
    margin-right: 2px;
    outline: none;
}

QToolBar QToolButton:hover{
    background-color: #EEEEEE;
    border: 0px solid #32414B;
    border-radius: 0px;
    margin-top: 2px;
    margin-bottom: 2px;
    margin-right: 2px;
    outline: none;
}


/* 输入框 */
QAbstractSpinBox {
    background-color: #F5F5F5;
    border: 1px solid #808080;
    color: #3F3F3F;
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 4px;
    padding-right: 4px;
    border-radius: 4px;
}

QAbstractSpinBox:up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    border-left: 1px solid #808080;
    margin: 1px;
}

QAbstractSpinBox::up-arrow,
QAbstractSpinBox::up-arrow:disabled,
QAbstractSpinBox::up-arrow:off {
    image: url(:/Icon/up_arrow_disabled);
    width: 9px;
    height: 9px;
}

QAbstractSpinBox::up-arrow:hover {
    image: url(:/Icon/up_arrow);
}

QAbstractSpinBox:down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    border-left: 1px solid #808080;
    margin: 1px;
}

QAbstractSpinBox::down-arrow,
QAbstractSpinBox::down-arrow:disabled,
QAbstractSpinBox::down-arrow:off {
    image: url(:/Icon/down_arrow_disabled);
    width: 9px;
    height: 9px;
}

QAbstractSpinBox::down-arrow:hover {
    image: url(:/Icon/down_arrow);
}

QAbstractSpinBox:hover{
    border: 1px solid #148CD2;
    color: #3F3F3F;
}

QAbstractSpinBox:selected {
    background: #1464A0;
    color: #808080;
}


/* 标签 */
QLabel {
    border: 0px solid #32414B;
    margin: 0px;
    color: #3F3F3F;
}

QLabel::disabled {
    border: 0px solid #32414B;
    color: #787878;
}


/* 复选框 */
QComboBox {
    border: 1px solid #808080;
    border-radius: 4px;
    selection-background-color: #1464A0;
    padding-top: 2px;     /* This fix  #103, #111*/
    padding-bottom: 2px;  /* This fix  #103, #111*/
    padding-left: 4px;
    padding-right: 4px;
    background-color: #F5F5F5;
    /* min-width: 75px;  removed to fix 109 */
}

QComboBox:disabled {
    background-color: #F5F5F5;
    color: #787878;
}

QComboBox:hover{
    border: 1px solid #148CD2;
}

QComboBox:on {
    selection-background-color: #F5F5F5;
}

QComboBox QAbstractItemView {
    background-color: #F5F5F5;
    border-radius: 4px;
    border: 1px solid #808080;
    selection-color: #3F3F3F;
    selection-background-color: #F5F5F5;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left-width: 0px;
    border-left-color: #808080;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
    image: url(:/Icon/down_arrow_disabled);
}

QComboBox::down-arrow:on,
QComboBox::down-arrow:hover,
QComboBox::down-arrow:focus {
    image: url(:/Icon/down_arrow);
}


/* Tab条 */
QTabBar {
    qproperty-drawBase: 0;
    border-radius: 4px;
    margin: 0px;
    padding: 2px;
    border: 0;

    /* left: 5px; move to the right by 5px - removed for fix */
    }

QTabBar::close-button {
    border: 0;
    margin: 2px;
    padding: 0;
    image: url(:/Icon/close);
}

QTabBar::close-button:hover {
    image: url(:/Icon/close-hover);
}

QTabBar::close-button:pressed {
    image: url(:/Icon/close-pressed);
}

QTabBar::tab:top:selected:disabled {
    border-bottom: 3px solid #14506E;
    color: #787878;
}

QTabBar::tab:bottom:selected:disabled {
    border-top: 3px solid #14506E;
    color: #787878;
}

QTabBar::tab:left:selected:disabled {
    border-left: 3px solid #14506E;
    color: #787878;
}

QTabBar::tab:right:selected:disabled {
    border-right: 3px solid #14506E;
    color: #787878;
}

QTabBar::tab:top:!selected:disabled {
    border-bottom: 3px solid #19232D;
    color: #787878;
    background-color: #19232D;
}

QTabBar::tab:bottom:!selected:disabled {
    border-top: 3px solid #19232D;
    color: #787878;
    background-color: #19232D;
}

QTabBar::tab:left:!selected:disabled {
    border-right: 3px solid #19232D;
    color: #787878;
    background-color: #19232D;
}

QTabBar::tab:right:!selected:disabled {
    border-left: 3px solid #19232D;
    color: #787878;
    background-color: #19232D;
}

QTabBar::tab:top:!selected {
    border-bottom: 2px solid #19232D;
    margin-top: 2px;
}

QTabBar::tab:bottom:!selected {
    border-top: 2px solid #19232D;
    margin-bottom: 3px;
}

QTabBar::tab:left:!selected {
    border-left: 2px solid #19232D;
    margin-right: 2px;
}

QTabBar::tab:right:!selected {
    border-right: 2px solid #19232D;
    margin-left: 2px;
}

QTabBar::tab:top {
    color: #3F3F3F;
    margin-left: 2px;
    padding-left: 4px;
    padding-right: 4px;
    padding-top: 2px;
    padding-bottom: 2px;
    min-width: 5px;
    border-bottom: 3px solid #808080;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
}

QTabBar::tab:top:selected {
    color: #3F3F3F;
    border-bottom: 1px solid #1464A0;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
}

QTabBar::tab:top:!selected:hover {
    border: 1px solid #148CD2;
    border-bottom: 3px solid #148CD2;
    padding: 0px;
}

QTabBar::tab:bottom {
    color: #3F3F3F;
    border-top: 3px solid #808080;
    margin-left: 2px;
    padding-left: 4px;
    padding-right: 4px;
    padding-top: 2px;
    padding-bottom: 2px;
    border-bottom-left-radius: 3px;
    border-bottom-right-radius: 3px;
    min-width: 5px;
}

QTabBar::tab:bottom:selected {
    color: #3F3F3F;
    border-top: 1px solid #1464A0;
    border-bottom-left-radius: 3px;
    border-bottom-right-radius: 3px;
}

QTabBar::tab:bottom:!selected:hover {
    border: 1px solid #148CD2;
    border-top: 3px solid #148CD2;
    padding: 0px;
}

QTabBar::tab:left {
    color: #3F3F3F;
    margin-top: 2px;
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 4px;
    padding-bottom: 4px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    min-height: 5px;
}

QTabBar::tab:left:selected {
    color: #3F3F3F;
    border-left: 1px solid #1464A0;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QTabBar::tab:left:!selected:hover {
    border: 1px solid #148CD2;
    border-left: 3px solid #148CD2;
    padding: 0px;
}

QTabBar::tab:right {
    color: #3F3F3F;
    margin-top: 2px;
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 4px;
    padding-bottom: 4px;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    min-height: 5px;
}

QTabBar::tab:right:selected {
    color: #3F3F3F;
    background-color: #505F69;
    border-right: 1px solid #1464A0;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
}

QTabBar::tab:right:!selected:hover {
    border: 1px solid #148CD2;
    border-right: 3px solid #148CD2;
    padding: 0px;
}

QTabBar QToolButton::right-arrow:enabled {
    image: url(:/Icon/right_arrow);
}

QTabBar QToolButton::left-arrow:enabled {
    image: url(:/Icon/left_arrow);
}

QTabBar QToolButton::right-arrow:disabled {
    image: url(:/Icon/right_arrow_disabled);
}

QTabBar QToolButton::left-arrow:disabled {
    image: url(:/Icon/left_arrow_disabled);
}

/* 滑动条 */
QSlider:disabled {
    background: #19232D;
}

QSlider:focus {
    border: none;
}

QSlider::groove:horizontal {
    background: #9DA5B4;
    border: 0px solid #32414B;
    height: 4px;
    margin: 0px;
    border-radius: 4px;
}

QSlider::sub-page:horizontal {
    background: #1464A0;
    border: 0px solid #32414B;
    height: 4px;
    margin: 0px;
    border-radius: 4px;
}

QSlider::sub-page:horizontal:disabled {
    background: #14506E;
}

QSlider::handle:horizontal {
    background: #787878;
    border: 0px solid #32414B;
    width: 8px;
    height: 8px;
    margin: -8px 0;
    border-radius: 4px;
}

QSlider::handle:horizontal:hover {
    background: #148CD2;
    border: 0px solid #148CD2;
}

QSlider::groove:vertical {
    background: #9DA5B4;
    border: 0px solid #32414B;
    width: 4px;
    margin: 0px;
    border-radius: 4px;
}

QSlider::sub-page:vertical {
    background: #1464A0;
    border: 0px solid #32414B;
    width: 4px;
    margin: 0px;
    border-radius: 4px;
}

QSlider::sub-page:vertical:disabled {
    background: #14506E;
}

QSlider::handle:vertical {
    background: #787878;
    border: 0px solid #32414B;
    width: 8px;
    height: 8px;
    margin: 0 -8px;
    border-radius: 4px;
}

QSlider::handle:vertical:hover {
    background: #148CD2;
    border: 0px solid #148CD2;
}


/* 文本输入 */
QLineEdit {
    background-color: #F5F5F5;
    padding-top: 2px;     /* This QLineEdit fix  103, 111 */
    padding-bottom: 2px;  /* This QLineEdit fix  103, 111 */
    padding-left: 4px;
    padding-right: 4px;
    border-style: solid;
    border: 1px solid #808080;
    border-radius: 4px;
    color: #3F3F3F;
}

QLineEdit:disabled {
    background-color: #F5F5F5;
    color: #787878;
}

QLineEdit:hover{
    border: 1px solid #148CD2;
    color: #3F3F3F;
}

QLineEdit:selected{
    background: #1464A0;
    color: #808080;
}


/* 框 */
QFrame {
    border-radius: 4px;
    border: 1px solid #808080;
}

QFrame[frameShape="0"] {
    border-radius: 4px;
    border: 1px transparent #808080;
}

QFrame[height="3"],
QFrame[width="3"] {
    background-color: #F5F5F5;
}


/* Tab组件 */
QTabWidget {
    padding: 2px;
    selection-background-color: #808080;
}

QTabWidget QFrame{
    border: 0;
}

QTabWidget::pane {
    border: 1px solid #808080;
    border-radius: 4px;
    padding: 2px;
    margin: 0px;
}

QTabWidget::pane:selected {
    background-color: #808080;
    border: 1px solid #1464A0;
}


/* 工具组件 */
#ContourWidgetList {
    background-color: #F5F5F5;
    border-radius: 10px;
}

#SeedsWidgetList {
    background-color: #F5F5F5;
    border-radius: 10px;
}

#LandMarkWidgetList {
    background-color: #F5F5F5;
    border-radius: 10px;
}

#PolyDeformationWidgetList {
    background-color: #F5F5F5;
    border-radius: 10px;
}

#ROIWidgetList {
    background-color: #F5F5F5;
    border-radius: 10px;
}


/* 表格组件 */
QTableWidget {
    background-color: #F5F5F5;
    color: #3F3F3F;
}

QHeaderView:section {
    background-color: #F5F5F5;
    color: #3F3F3F;
}

QHeaderView {
    background-color: #F5F5F5;
    color: #3F3F3F;
}

/* 菜单 */
QMenu {
    background-color: #F5F5F5;
    color: #3F3F3F;
}


/* 树型View */
QTreeView:branch:selected,
QTreeView:branch:hover {
    background: url(:/Icon/transparent);
}

QTreeView::branch:has-siblings:!adjoins-item {
    border-image: url(:/Icon/transparent);
}

QTreeView::branch:has-siblings:adjoins-item {
    border-image: url(:/Icon/transparent);
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item {
    border-image: url(:/Icon/transparent);
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
    image: url(:/Icon/branch_closed);
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings {
    image: url(:/Icon/branch_open);
}

QTreeView::branch:has-children:!has-siblings:closed:hover,
QTreeView::branch:closed:has-children:has-siblings:hover {
    image: url(:/Icon/branch_closed-on);
}

QTreeView::branch:open:has-children:!has-siblings:hover,
QTreeView::branch:open:has-children:has-siblings:hover {
    image: url(:/Icon/branch_open-on);
}

QTreeView::indicator:checked,
QTableView::indicator:checked,
QListView::indicator:checked {
    image: url(:/Icon/checkbox_checked);
}

QTreeView::indicator:unchecked,
QTableView::indicator:unchecked,
QListView::indicator:unchecked {
    image: url(:/Icon/checkbox_unchecked);
}

QTreeView::indicator:checked:hover,
QTreeView::indicator:checked:focus,
QTreeView::indicator:checked:pressed,
QTableView::indicator:checked:hover,
QTableView::indicator:checked:focus,
QTableView::indicator:checked:pressed,
QListView::indicator:checked:hover,
QListView::indicator:checked:focus,
QListView::indicator:checked:pressed {
    image: url(:/Icon/checkbox_checked_focus);
}

QTreeView::indicator:unchecked:hover,
QTreeView::indicator:unchecked:focus,
QTreeView::indicator:unchecked:pressed,
QTableView::indicator:unchecked:hover,
QTableView::indicator:unchecked:focus,
QTableView::indicator:unchecked:pressed,
QListView::indicator:unchecked:hover,
QListView::indicator:unchecked:focus,
QListView::indicator:unchecked:pressed {
    image: url(:/Icon/checkbox_unchecked_focus);
}

QTreeView::indicator:indeterminate:hover,
QTreeView::indicator:indeterminate:focus,
QTreeView::indicator:indeterminate:pressed,
QTableView::indicator:indeterminate:hover,
QTableView::indicator:indeterminate:focus,
QTableView::indicator:indeterminate:pressed,
QListView::indicator:indeterminate:hover,
QListView::indicator:indeterminate:focus,
QListView::indicator:indeterminate:pressed {
    image: url(:/Icon/checkbox_indeterminate_focus);
}

QTreeView::indicator:indeterminate,
QTableView::indicator:indeterminate,
QListView::indicator:indeterminate {
    image: url(:/Icon/checkbox_indeterminate);
}


/* 文本浏览器 */
#textBrowser,#textBrowser_2,#textBrowser_3,#textBrowser_4,#textBrowser_5 {
    background-color: #F5F5F5;
    border: 0px solid #32414B;
    padding: 0px;
    color: #3F3F3F;
    selection-background-color: #F5F5F5;
    selection-color: #3F3F3F;
}

QTreeView {
    selection-background-color: #EEEEEE;
    selection-color: #EEEEEE;
}
QTreeView:active {
    selection-background-color: #EEEEEE;
    selection-color: #EEEEEE;
}


QTextBrowser {
    background-color: #F5F5F5;
    border: 1px solid #32414B;
    color: #3F3F3F;
    border-radius: 4px;
}

QTextBrowser:disabled {
    background-color: #F5F5F5;
    border: 1px solid #32414B;
    color: #787878;
    border-radius: 4px;
}

QTextBrowser:hover,
QTextBrowser:!hover,
QTextBrowser::selected,
QTextBrowser::pressed {
    border: 1px solid #32414B;
}